        "/login": {
            "post": {
                "summary": "Login de usuário",
                "description": "Essa rota responsável pela autenticao do usuário no sistema, ela retorna um token",
                "tags": ["Login"],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "first_name": {
                                            "type": "string",
                                            "example": "Marcia"
                                        },
                                        "last_name": {
                                            "type": "string",
                                            "example": "Silva"
                                        },
                                        "email": {
                                            "type": "string",
                                            "example": "marcia@teste.com.br"
                                        },
                                        "photo": {
