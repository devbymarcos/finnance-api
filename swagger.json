                    }
                }
            }
        },
        "/wallet": {
            "get": {
                "summary": "Retorna todas as carteiras",
                "description": "Essa rota retorna todas as carteiras registradas",
                "tags": ["Wallet"],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "id": {
                                            "type": "integer",
                                            "example": "1"
                                        },
                                        "user_id": {
                                            "type": "integer",
                                            "example": "2"
                                        },
                                        "name": {
                                            "type": "string",
                                            "example": "name wallet"
                                        },
                                        "description": {
                                            "type": "string",
                                            "example": "description wallet"
                                        },
                                        "option_wallet": {
                                            "type": "integer",
                                            "example": "1 or 0 -significa ativo or inativo como preferencia do sistema"
                                        },
                                        "created_at": {
                                            "type": "string",
                                            "example": "2023-10-01T14:30:18.000Z"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Não foi encontrado dados, faça um registro ",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Não econstramos a wallet solicitada"
                                        },
                                        "data": {
                                            "type": "object",
                                            "example": null
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            },
            "post": {
                "summary": "Cria uma nova carteira",
                "tags": ["Wallet"],
                "description": "Rota para criar uma nova carteira",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "minha Carteira"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "carteira principal"
                                    },
                                    "option_wallet": {
                                        "type": "integer",
                                        "example": "1 or 0 -significa ativo or inativo como preferencia do sistema"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "id": {
                                            "type": "integer",
                                            "example": "1"
                                        },
                                        "user_id": {
                                            "type": "integer",
                                            "example": "2"
                                        },
                                        "name": {
                                            "type": "string",
                                            "example": "name wallet"
                                        },
                                        "description": {
                                            "type": "string",
                                            "example": "description wallet"
                                        },
                                        "option_wallet": {
                                            "type": "integer",
                                            "example": "1 or 0 -significa ativo or inativo como preferencia do sistema"
                                        },
                                        "created_at": {
                                            "type": "string",
                                            "example": "2023-10-01T14:30:18.000Z"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/wallet/{id}": {
            "get": {
                "summary": "Busca por 'id'",
                "tags": ["Wallet"],
                "description": "Essa rota retorna todas as carteiras registradas",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Id da carteira",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "name": {
                                            "type": "string",
                                            "example": "minha Carteira"
                                        },
                                        "description": {
                                            "type": "string",
                                            "example": "carteira principal"
                                        },
                                        "option_wallet": {
                                            "type": "integer",
                                            "example": "1 or 0 -significa ativo or inativo como preferencia do sistema"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "put": {
                "summary": "Atualiza carteira",
                "tags": ["Wallet"],
                "responses": {}
            },
            "delete": {
                "summary": "Remove carteira",
                "tags": ["Wallet"],
                "responses": {}
            }
